{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "adw/config-bundle": "^1.4",
        "doctrine/annotations": "^1.6",
        "doctrine/common": "^2.8",
        "doctrine/dbal": "^2.6",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.10",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/user-bundle": "^2.1",
        "nelmio/api-doc-bundle": "^3.1",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "^3.33",
        "sonata-project/block-bundle": "^3.12",
        "sonata-project/doctrine-orm-admin-bundle": "^3.4",
        "sonata-project/intl-bundle": "^2.4",
        "sonata-project/media-bundle": "^3.11",
        "sonata-project/user-bundle": "^4.1",
        "symfony/console": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-pack": "^1.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/functional-test-bundle": "^1.9",
        "sensiolabs/security-checker": "^4.1",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "adw/*": "source",
            "*": "auto"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony-tests-dir": "tests",
        "symfony-bin-dir": "bin",
        "symfony": {
            "id": "01C918A7SYKJHGP40KCR0021AW",
            "allow-contrib": false
        }
    }
}
